{
  "compilerOptions": {
    "target": "es2023",
    "module": "commonjs",
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      "@prisma/*": ["./prisma/*"]
    },
    "types": ["vitest/globals"],
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}